{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md", "**/toc.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
	"breadcrumb_path": "/CATS_Document/breadcrumb/toc.json"
	},
    "fileMetadata": {},
    "template": [],
    "dest": "CATS_Document"
  }
}